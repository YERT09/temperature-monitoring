@model IEnumerable<Proyecto.Models.Dispositivo>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    int cont = 1;
    Layout = "";
}
<div class="row">
    @foreach (var device in Model)
    {
        <div id="@device.Id"class="col-sm-12 col-md-4">
            <div id="@device.Id-graf" class="graf">
            </div>
            <div class="device">
                @device.Nombre
            </div>
            <div class="temperatura">
                Temperatura actual:
            </div>
            <div class="rango">
                rango
            </div>
            <div class="fecha">
                fecha
            </div>

        </div>
        cont++;
    }
</div>